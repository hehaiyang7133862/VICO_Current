<UserControl x:Class="nsVicoClient.ctrls.motorOnBtn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="74" d:DesignWidth="74"
             xmlns:my="clr-namespace:nsVicoClient.ctrls">
    <Canvas Height="74" Width="74" Name="cvsMain" MouseDown="cvsMain_MouseDown" MouseUp="cvsMain_MouseUp" MouseLeave="cvsMain_MouseLeave">
        <Image Height="74" Width="74" Source="/Program;component/png/btn/OBJ_13_3_MidButtonbaseRelease_V0.0.png" Name="btnBg"/>
        <Image Height="74" Width="74"  Source="/Program;component/png/btn/OBJ_13_4_MidButtonbaseActive_V0.0.png" Name="btnDown" Visibility="Hidden"/>
        <Image Height="74" Width="74" Source="/Program;component/png/btn/OBJ_5_15_MotorStop_V0.0.png" Name="BtnFore" />
        <Image Height="74" Width="74" Source="/Program;component/png/btn/OBJ_5_13_MotorStart_V0.0.png" Name="BtnForeActive" />
        
        <my:MotorOnProcessBar rateValue="100" ForceGround="#FF00C34E" x:Name="rate" Canvas.Left="-5" Canvas.Top="-5" />
    </Canvas>
</UserControl>
