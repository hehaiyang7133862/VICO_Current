<UserControl x:Class="nsVicoClient.ctrls.btmBtn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="68" d:DesignWidth="68">
    <Canvas Height="68" Width="68" Name="cvsMain">
        <Image Height="68" Width="68" Source="{StaticResource imgKeyNullBg}" Name="btnBg"/>
        <Image Height="68" Width="68" Source="{StaticResource imgKeyPressed}" Name="btnDown" Visibility="Hidden"/>
        <Image Height="68" Width="68" Name="BtnActive" Visibility="Hidden"/>
        <Ellipse Height="53" Width="53" Fill="LightBlue" Opacity="0" Canvas.Left="8" Canvas.Top="8" Name="focusCtrl" MouseDown="focusCtrl_MouseDown" MouseUp="focusCtrl_MouseUp" MouseLeave="focusCtrl_MouseLeave" />
    </Canvas>

</UserControl>
