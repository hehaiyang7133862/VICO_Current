<Window x:Class="nsVicoClient.ctrls.ConfigOutPut"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ConfigOutPut" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Grid.Column="1" Name="lbPath" />
        <Button Content="Button" Height="34" Width="80" Click="Button_Click" />
        <ProgressBar Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Name="ProgressBar" Height="35" Width="500"/>
        <RichTextBox Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Name="tbResult" />
    </Grid>
</Window>
