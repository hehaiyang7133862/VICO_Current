             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200" 
             xmlns:my="clr-namespace:nsVicoClient.ctrls">
