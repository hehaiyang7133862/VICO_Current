<UserControl x:Class="nsVicoClient.ctrls.startStateCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="46" d:DesignWidth="20">
    <Canvas Height="46" Width="20">
        <Canvas Height="15" Width="10" Canvas.Left="10" Canvas.Top="8">
            <Rectangle Name="rctUp" Width="4" Height="11"  Fill="Red"  Canvas.Left="3" Canvas.Top="4" Stroke="{x:Null}" />
            <Polygon Name="pgUp" Points="5,0 10,5 0,5" Fill="Red"  Canvas.Left="0" Canvas.Top="0"/>
        </Canvas>
        <Canvas Height="10" Width="15" Canvas.Top="18">
            <Rectangle Name="pgLeft" Width="11" Height="4"  Fill="Red"  Canvas.Left="4" Canvas.Top="3" />
            <Polygon Name="rctLeft" Points="5,0 5,10 0,5"  Fill="Red"  Canvas.Top="0" Canvas.Left="0" />
        </Canvas>
        <Canvas Height="15" Width="10" Canvas.Top="23" Canvas.Left="10">
            <Rectangle Name="pgDown" Width="4" Height="11"  Fill="Red"  Canvas.Left="3" />
            <Polygon Name="rctDown" Points="0,0 10,0 5,5"  Fill="Red"  Canvas.Top="10" Canvas.Left="0" />
        </Canvas>
    </Canvas>
</UserControl>
